{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "analytics": false
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "tslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "tslint"
      },
      "library": {
        "linter": "tslint"
      }
    },
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "tslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "scss",
      "linter": "tslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "ebook",
  "projects": {
    "ebook": {
      "projectType": "application",
      "root": "apps/ebook",
      "sourceRoot": "apps/ebook/src",
      "prefix": "assignment",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/ebook",
            "index": "apps/ebook/src/index.html",
            "main": "apps/ebook/src/main.ts",
            "polyfills": "apps/ebook/src/polyfills.ts",
            "tsConfig": "apps/ebook/tsconfig.app.json",
            "aot": true,
            "assets": ["apps/ebook/src/favicon.ico", "apps/ebook/src/assets"],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "apps/ebook/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/ebook/src/environments/environment.ts",
                  "with": "apps/ebook/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "ebook:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "ebook:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ebook:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/ebook/tsconfig.app.json",
              "apps/ebook/tsconfig.spec.json",
              "apps/ebook/tsconfig.editor.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/ebook/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ebook"],
          "options": {
            "jestConfig": "apps/ebook/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ebook-e2e": {
      "root": "apps/ebook-e2e",
      "sourceRoot": "apps/ebook-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ebook-e2e/cypress.json",
            "tsConfig": "apps/ebook-e2e/tsconfig.e2e.json",
            "devServerTarget": "ebook:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ebook:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/ebook-e2e/tsconfig.e2e.json"],
            "exclude": ["**/node_modules/**", "!apps/ebook-e2e/**/*"]
          }
        }
      }
    }
  }
}
